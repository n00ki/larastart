{
  "type": "module",
  "private": true,
  "scripts": {
    "build": "vite build",
    "build:ssr": "vite build && vite build --ssr",
    "dev": "vite",
    "format": "prettier --write resources/",
    "format:check": "prettier --check resources/",
    "check": "svelte-check resources/ --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check resources/ --tsconfig ./tsconfig.json --watch",
    "lint": "eslint . && svelte-check resources/ --tsconfig ./tsconfig.json",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {},
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.50.2",
    "@tailwindcss/oxide-linux-x64-gnu": "^4.1.13",
    "lightningcss-linux-x64-gnu": "^1.30.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.36.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@inertiajs/svelte": "^2.1.6",
    "@internationalized/date": "^3.9.0",
    "@laravel/vite-plugin-wayfinder": "^0.1.3",
    "@lucide/svelte": "^0.544.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/table-core": "^8.21.3",
    "@tsconfig/svelte": "^5.0.5",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.5.2",
    "axios": "^1.12.2",
    "bits-ui": "^2.9.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-svelte": "^8.6.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.3",
    "eslint-plugin-unicorn": "^61.0.2",
    "formsnap": "^2.0.1",
    "globals": "^16.4.0",
    "laravel-vite-plugin": "^2.0.1",
    "layerchart": "^1.0.12",
    "lint-staged": "^16.1.6",
    "maildev": "^2.2.1",
    "paneforge": "^1.0.2",
    "playwright": "^1.55.1",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "simple-git-hooks": "^2.13.1",
    "svelte": "^5.39.2",
    "svelte-check": "^4.3.1",
    "svelte-portal": "^2.2.1",
    "svelte-sonner": "^1.0.5",
    "svelte-transition": "^0.0.17",
    "sveltekit-superforms": "^2.27.1",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.13",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.45.0",
    "vaul-svelte": "^0.3.2",
    "vite": "^7.1.6",
    "ziggy-js": "^2.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "bun x lint-staged"
  },
  "lint-staged": {
    "**/*.php": [
      "./vendor/bin/pint"
    ],
    "{app,bootstrap,database,public}/**/*.php": [
      "./vendor/bin/rector"
    ],
    "app/**/*.php": [
      "./vendor/bin/phpstan analyse -c ./phpstan.neon --error-format=table"
    ],
    "resources/**/*.{js,ts,svelte,css,json,md}": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  }
}
