{
    "mcpServers": {
        "laravel-boost": {
            "command": "php",
            "args": [
                "artisan",
                "boost:mcp"
            ]
        },
        "svelte": {
            "command": "npx",
            "args": [
                "-y",
                "@sveltejs/mcp"
            ]
        }
    }
}